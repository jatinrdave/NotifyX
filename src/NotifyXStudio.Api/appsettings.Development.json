{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "RateLimiting": {
    "Enabled": false
  },
  "RequestLogging": {
    "LogBody": true
  },
  "ErrorHandling": {
    "IncludeStackTrace": true,
    "ShowDetailedErrors": true
  },
  "Caching": {
    "Enabled": false
  }
}