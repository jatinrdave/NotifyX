{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateLimiting": {
    "Enabled": true,
    "RequestsPerWindow": 50,
    "WindowMs": 60000
  },
  "RequestLogging": {
    "LogBody": false
  },
  "ErrorHandling": {
    "IncludeStackTrace": false,
    "ShowDetailedErrors": false
  },
  "Caching": {
    "Enabled": true,
    "DefaultCacheDurationSeconds": 600
  },
  "SecurityHeaders": {
    "CorsOrigin": "https://yourdomain.com"
  }
}